
<% if current_partner %>
	<% if current_partner.has_logo? %>
		<%= image_tag current_partner.logo.url(:icon_340_74), :border => 0 %>
	<% else %>
		<% if current_partner.has_website? and current_partner.has_logo? %>
			<a href="<%= current_partner.website_link %>"><%= image_tag current_partner.logo.url(:icon_180), :border => 0 %></a>		
		<% elsif current_partner.has_logo? and not current_partner.has_website? %>
			<%= image_tag current_partner.logo.url(:icon_214_32), :border => 0 %>
		<% elsif logged_in? and current_user.partner_id == current_partner.id %>
			<% if current_partner.has_website? %>
				<a href="<%= current_partner.website_link %>"><%= h(current_partner.name) %></a>
			<% else %>
				<%= h(current_partner.name) %>
			<% end %>
			<p><%= link_to (image_tag("your_logo.gif", :size => "180x100", :border => 0)), picture_partner_path(current_partner) %>
		<% else %>
			<p>
			Partnered with
			<% if current_partner.has_website? %>
				<a href="<%= current_partner.website_link %>"><%= h(current_partner.name) %></a>.
			<% else %>
				<%= h(current_partner.name) %>.
			<% end %>	
		<% end %>
	<% end %>
<% end %>